<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <springProfile name="dev">
    <appender class="ch.qos.logback.core.ConsoleAppender" name="CONSOLE">
      <encoder>
        <pattern>%date [%thread] %-5level %logger{80} || %msg%n</pattern>
      </encoder>
    </appender>
    <root level="info">
      <appender-ref ref="CONSOLE"/>
    </root>
  </springProfile>
</configuration>
